{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",

  "importOrder": [
    "<THIRD_PARTY_MODULES>",
		"^@/components/(.*)$",
		"^@/components/layout/(.*)$",
		"^@/components/screens/(.*)$",
		"^@/components/ui/(.*)$",
		"^@/styles/(.*)$",
		"^@/config/(.*)$",
		"^@/utils/(.*)$",
		"^@/services/(.*)$",
		"^.+\\.(module\\.scss)$",
		"^../(.*)",
		"^./(.*)"
],
"importOrderSeparation": true,
"importOrderSortSpecifiers": true
}